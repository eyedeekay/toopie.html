{
  "browser_specific_settings": {
    "gecko": {
      "id": "toopie@eyedeekay.github.io",
      "strict_min_version": "91.1.0"
    }
  },
  "permissions": [
    "proxy",
    "<all_urls>"
  ],
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "2.7.1",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/eyedeekay/toopie.html",
  "icons": {
    "96": "images/itoopie.svg"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "images/itoopie.svg",
    "default_title": "__MSG_extensionName__",
    "default_popup": "toopie.html",
    "default_area": "navbar"
  },
  "sidebar_action": {
    "browser_style": true,
    "default_icon": "images/itoopie.svg",
    "default_title": "__MSG_toopieName__",
    "default_panel": "toopie.html",
    "open_at_install": true
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "config.js"
    ]
  },
  "default_locale": "en"
}
